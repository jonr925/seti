				}
				if ( state == 'Assignment' ) {
					currentState='DataViz';
				}
				if ( state == 'DataViz' ) {
					currentState='Categorize';
				}
				if ( state == 'Signal' ) {
					currentState='DataViz';
				}
			}


			protected function button_clickHandler_1():void
			{
